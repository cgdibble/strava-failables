{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "single"]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "globals": {
    "const": true,
    "require": true,
    "describe": true,
    "it": true,
    "beforeAll": true,
    "process": true,
    "module": true
  }
}